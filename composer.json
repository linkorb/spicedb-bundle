{
    "name": "linkorb/spicedb-bundle",
    "description": "SpiceDB bundle for Symfony apps",
    "license": "MIT",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "LinkORB\\Bundle\\SpicedbBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LinkORB\\Bundle\\SpicedbBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "LinkORB"
        }
    ],
    "require": {
        "php": ">=7.4",
        "linkorb/spicedb-php": "^1.1",
        "symfony/dependency-injection": "^4.4|^5.4|^6.4|^7.2",
        "symfony/config": "^4.4|^5.4|^6.4|^7.2",
        "symfony/http-kernel": "^4.4|^5.4|^6.4|^7.2",
        "symfony/yaml": "^4.4|^5.4|^6.4|^7.2",
        "symfony/framework-bundle": "^4.4|^5.4|^6.4|^7.2",
        "symfony/security-core": "^4.4|^5.4|^6.4|^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/test-pack": "^1.0"
    }
}
